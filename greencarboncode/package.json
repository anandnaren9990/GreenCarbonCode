{
  "name": "greencarboncode",
  "displayName": "greenCarbonCode",
  "description": "This extension is to suggest a carbon  effective version of code...",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.106.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onDidChangeTextDocument"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "carbon.analyzeCurrentFile",
        "title": "Carbon: Analyze Current File"
      },
      {
        "command": "carbon.generateGreenerCode",
        "title": "Carbon: Generate Greener Code Suggestion"
      },
      {
        "command": "carbon.showInsights",
        "title": "Carbon: Show Insights Dashboard"
      },
      {
        "command": "carbon.clearHistory",
        "title": "Carbon: Clear History"
      },
      {
        "command": "carbon.clearCodeCache",
        "title": "Carbon: Clear Code Cache"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/esprima": "^4.0.6",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.106.0",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.39.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.3"
  },
  "dependencies": {
    "@babel/parser": "^7.28.5",
    "esprima": "^4.0.1",
    "java-parser": "^3.0.1",
    "node-fetch": "^3.3.2"
  },

  "extensionDependencies": [
    "GitHub.copilot"
  ]
}
